{
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "Streamer.Identity.Client": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "TokenSettings":{
    "Issuer":"localhost:5000",
    "Audience":"API",
    "Key":"MySecuredKey12345678910MySecuredKey12345678910"
  },
"AllowedHosts": "*"
}
